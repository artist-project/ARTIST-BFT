  <org.eclipse.epf.uma:ProcessDescription xmi:id="-26wT7axqn57rwKSF11XnRA" name="Cloud provider management,_RgojUfRAEeKzY_EmAQPgmw" guid="-26wT7axqn57rwKSF11XnRA">
